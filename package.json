{
  "name": "Feed and Sleep",
  "version": "2.1.1",
  "description": "Capacitor wrapped react app to provide a simple solution of baby feeding timing",
  "main": "index.tsx",
  "author": "Tobias Justinger",
  "license": "MIT",
  "homepage": ".",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --progress --port 3000 --mode=development",
    "start:local": "cross-env NODE_ENV=development CAPACITOR_LOCAL_URL=http://localhost:3000 npm start",
    "start:network": "cross-env NODE_ENV=development CAPACITOR_LOCAL_URL=http://192.168.178.22:3000 npm start",
    "test": "test",
    "build": "cross-env NODE_ENV=production webpack build --mode=production",
    "build:dev": "cross-env NODE_ENV=development webpack build --mode=development",
    "generate:config": "ts-node scripts/generate-capacitor-config.ts",
    "sync:ios": "npm run build && cross-env NODE_ENV=production npm run generate:config && npx cap sync ios",
    "sync:android": "npm run build && cross-env NODE_ENV=production npm run generate:config && npx cap sync android",
    "sync:ios:dev": "cross-env NODE_ENV=development CAPACITOR_LOCAL_URL=http://192.168.178.22:3000 sh -c 'npm run build:dev && npm run generate:config && npx cap sync ios'",
    "sync:ios:local": "cross-env NODE_ENV=development CAPACITOR_LOCAL_URL=http://localhost:3000 sh -c 'npm run build:dev && npm run generate:config && npx cap sync ios'",
    "sync:android:dev": "cross-env NODE_ENV=development CAPACITOR_LOCAL_URL=http://192.168.178.22:3000 sh -c 'npm run build:dev && npm run generate:config && npx cap sync android'",
    "lint": "npx eslint ./src",
    "lint:fix": "npx eslint ./src  --fix",
    "bump:major": "npm version major -m 'v%s'",
    "bump:minor": "npm version minor -m 'v%s'",
    "bump:patch": "npm version patch -m 'v%s'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postdeploy": "rm -rf build",
    "open:ios": "npx cap open ios",
    "run:ios": "npx cap run ios"
  },
  "dependencies": {
    "@capacitor-community/admob": "^7.0.1",
    "@capacitor-community/in-app-review": "^7.0.0",
    "@capacitor-community/keep-awake": "^7.0.0",
    "@capacitor-community/sqlite": "^7.0.0",
